set $term foot
set $browser MOZ_ENABLE_WAYLAND=1 librewolf
set $filemanager nautilus
# Because somehow /etc/environment does not take effect on emacs
set $editor emacs
set $launcher dmenu_path | dmenu | xargs swaymsg exec --
set $exit echo "poweroff\nsuspend\nreboot" | dmenu | xargs systemctl --
set $idle 600
set $dmps_off 660
